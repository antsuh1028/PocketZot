{
  "manifest_version": 3,
  "name": "PocketZot",
  "version": "0.1.0",
  "description": "A lightweight Chrome extension starter for PocketZot.",
  "action": {
    "default_title": "PocketZot",
    "default_popup": "dist/pocket_zot.html"
  },
  "permissions": ["storage"],
  "host_permissions": ["http://127.0.0.1:8000/*"]
}